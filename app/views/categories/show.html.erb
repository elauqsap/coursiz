<div class="container" id="mainNav">
  <div class="row-fluid">
    <div class="span2">
          <%= render 'layouts/sidenav' %>
        </div>
      <div class="span10">
          <table class="table table-bordered" id="catTable">
              <thead>
                <tr>
                  <th><%= sortable "quiz number" %></th>
                </tr>
              </thead>
              <tbody>
              <% @cat2.each do |cat| %>
                <tr>
                  <td id="<%= cat %>"><a href="/categories/<%= @cat.name%>/quizzes/<%= cat%>"><%= @url_text[cat-1] %></a></td>
                </tr>
              <% end %>
              </tbody>
            </table>

      </div>
    </div>
  </div>
</div>