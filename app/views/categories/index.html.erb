<div class="container" id="mainNav">
  <div class="row-fluid">
    <div class="span2">
          <%= render 'layouts/sidenav' %>
        </div>
      <div class="span8">
          <table class="table table-bordered animated fadeIn">
              <thead>
                <tr>
                  <th><%= sortable "name" %></th>
                  <th><%= sortable "created_at", "Created" %></th>
                </tr>
              </thead>
              <tbody>
              <% @cat.each do |cat| %>
                <tr id="category" class="<%= cat.name%>" >
                  <td id="<%= cat.name %>"><a href="categories/<%= cat.name%>"><i class="icon-chevron-right"></i>  <%= cat.real_name %></a></td>
                  <td id="number"><%= cat.created_at.strftime("%b. %d, %Y") %></td>
                </tr>
              <% end %>
              </tbody>
            </table>

      </div>
    </div>
  </div>
</div>

